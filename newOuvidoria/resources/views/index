            <script>
                function resposta6(valor) {
                    $('#det6_container').css('display','block');
                    switch(parseInt(valor))	{
                        case 5: $('#det6_container').html('&raquo; Excelente'); break;
                        case 4: $('#det6_container').html('&raquo; Bom'); break;
                        case 3: $('#det6_container').html('&raquo; Regular'); break;
                        case 2: $('#det6_container').html('&raquo; Ruim'); break;
                        case 0: $('#det6_container').html('&raquo; Não desejo responder / Não se aplica'); break;
                    }
                }
                $(document).ready(function(){
                    $("input[name='p6']").change(function() {
                        $("#radio6img5").attr('src', 'images/tp6_ico_set1_5h.png').css({ 'margin':'4px', 'width':'24px', 'height':'24px' });
                        $("#radio6img4").attr('src', 'images/tp6_ico_set1_4h.png').css({ 'margin':'4px', 'width':'24px', 'height':'24px' });
                        $("#radio6img3").attr('src', 'images/tp6_ico_set1_3h.png').css({ 'margin':'4px', 'width':'24px', 'height':'24px' });
                        $("#radio6img2").attr('src', 'images/tp6_ico_set1_2h.png').css({ 'margin':'4px', 'width':'24px', 'height':'24px' });
                        $("#radio6img0").attr('src', 'images/tp6_ico_set1_0h.png').css({ 'margin':'4px', 'width':'24px', 'height':'24px' });
                        $("#radio6img" + $(this).attr('ordem')).attr('src', 'images/tp6_ico_set1_' + $(this).attr('ordem') + '.png').css({ 'margin':'0px', 'width':'32px', 'height':'32px' });
                        resposta6($(this).attr('ordem'));
                    });
                });
            </script>
